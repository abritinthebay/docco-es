{
	"env" : {
		"browser" : true,
		"commonjs" : true,
		"es6" : true,
		"node" : true
	},
	"extends" : [
		"eslint:recommended"
	],
	"parser" : "babel-eslint",
	"parserOptions" : {
		"ecmaFeatures" : {
			"arrowFunctions" : true,
			"binaryLiterals" : true,
			"blockBindings" : true,
			"classes" : true,
			"defaultParams" : true,
			"destructuring" : true,
			"experimentalObjectRestSpread" : true,
			"forOf" : true,
			"generators" : true,
			"globalReturn" : false,
			"jsx" : true,
			"modules" : true,
			"objectLiteralComputedProperties" : true,
			"objectLiteralDuplicateProperties" : true,
			"objectLiteralShorthandMethods" : true,
			"objectLiteralShorthandProperties" : true,
			"octalLiterals" : true,
			"regexUFlag" : true,
			"regexYFlag" : true,
			"restParams" : true,
			"spread" : true,
			"superInFunctions" : true,
			"templateStrings" : true,
			"unicodeCodePointEscapes" : true
		},
		"sourceType" : "module"
	},
	"plugins" : [
		"babel"
	],
	"rules" : {
		"accessor-pairs" : 2,
		"arrow-parens" : [2,"always"],
		"arrow-spacing" : 2,
		"babel/object-curly-spacing" : 1,
		"block-scoped-var" : 1,
		"block-spacing" : 2,
		"brace-style" : 2,
		"callback-return" : 0,
		"comma-dangle" : [2, "never"],
		"comma-spacing" : [2, {"after" : true, "before" : false}],
		"comma-style" : [2, "last"],
		"complexity" : 0,
		"computed-property-spacing" : [2, "never"],
		"consistent-return" : 1,
		"constructor-super" : 2,
		"curly" : 2,
		"default-case" : 2,
		"eol-last" : 2,
		"eqeqeq" : [ 2, "smart" ],
		"generator-star-spacing" : [2, {"after" : true, "before" : true}],
		"guard-for-in" : 2,
		"handle-callback-err" : 1,
		"id-length" : [2, {"max" : 32, "min" : 2, "properties" : "never"}],
		"indent" : [2, "tab", {"SwitchCase" : 1 }],
		"keyword-spacing" : [2, {"after" : true, "before" : true, "overrides" : {}}],
		"new-cap" : 2,
		"no-alert" : 2,
		"no-array-constructor" : 2,
		"no-await-in-loop": 2,
		"no-caller" : 2,
		"no-catch-shadow" : 0,
		"no-class-assign" : 2,
		"no-cond-assign" : [ 2, "except-parens"],
		"no-console" : 1,
		"no-const-assign" : 2,
		"no-constant-condition" : 2,
		"no-continue" : 2,
		"no-control-regex" : 1,
		"no-debugger" : 2,
		"no-delete-var" : 2,
		"no-div-regex" : 2,
		"no-dupe-args" : 2,
		"no-dupe-class-members" : 2,
		"no-dupe-keys" : 2,
		"no-duplicate-case" : 2,
		"no-else-return" : 2,
		"no-empty" : 2,
		"no-empty-character-class" : 1,
		"no-eq-null" : 2,
		"no-eval" : 2,
		"no-ex-assign" : 2,
		"no-extend-native" : 0,
		"no-extra-bind" : 2,
		"no-extra-boolean-cast" : 2,
		"no-extra-parens" : 2,
		"no-extra-semi" : 2,
		"no-floating-decimal" : 2,
		"no-func-assign" : 2,
		"no-global-assign": 0,
		"no-implied-eval" : 2,
		"no-inner-declarations" : 2,
		"no-invalid-regexp" : 2,
		"no-invalid-this" : 2,
		"no-irregular-whitespace" : 2,
		"no-iterator" : 2,
		"no-label-var" : 2,
		"no-labels" : 2,
		"no-lone-blocks" : 2,
		"no-lonely-if" : 2,
		"no-loop-func" : 2,
		"no-mixed-spaces-and-tabs" : [1, "smart-tabs"],
		"no-multi-spaces" : 2,
		"no-multi-str" : 2,
		"no-multiple-empty-lines" : 2,
		"no-native-reassign" : 0,
		"no-negated-in-lhs" : 2,
		"no-new" : 2,
		"no-new-func" : 2,
		"no-new-object" : 2,
		"no-new-require" : 1,
		"no-obj-calls" : 2,
		"no-octal" : 2,
		"no-octal-escape" : 2,
		"no-param-reassign" : 2,
		"no-path-concat" : 1,
		"no-process-env" : 0,
		"no-process-exit" : 1,
		"no-proto" : 2,
		"no-redeclare" : 2,
		"no-regex-spaces" : 1,
		"no-return-assign" : 2,
		"no-script-url" : 2,
		"no-self-compare" : 2,
		"no-shadow-restricted-names" : 2,
		"no-spaced-func" : 2,
		"no-sparse-arrays" : 2,
		"no-this-before-super" : 2,
		"no-throw-literal" : 2,
		"no-trailing-spaces" : 2,
		"no-undef" : 2,
		"no-undef-init" : 2,
		"no-undefined" : 1,
		"no-unexpected-multiline" : 2,
		"no-unneeded-ternary" : 2,
		"no-unreachable" : 2,
		"no-unused-expressions" : 0,
		"no-unused-vars" : [1, {
			"args" : "all",
			"argsIgnorePattern" : "^_",
			"caughtErrors" : "all",
			"vars" : "all"
		}],
		"no-use-before-define" : 2,
		"no-useless-call" : 2,
		"no-useless-concat" : 2,
		"no-var" : 0,
		"no-void" : 2,
		"no-with" : 0,
		"object-shorthand" : 1,
		"padded-blocks" : [1, "never"],
		"prefer-const": 2,
		"prefer-spread" : 1,
		"prefer-template" : 1,
		"quotes" : [1, "double"],
		"radix" : 0,
		"require-yield" : 2,
		"semi" : [2, "always"],
		"space-before-function-paren" : [2, "never"],
		"space-infix-ops" : [2, {"int32Hint" : false }],
		"space-unary-ops" : [1, {"nonwords" : false, "words" : true}],
		"use-isnan" : 2,
		"valid-typeof" : 2,
		"vars-on-top" : 1,
		"wrap-iife" : 2,
		"wrap-regex" : 1
	}
}
