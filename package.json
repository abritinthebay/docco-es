{
	"name": "docco-es",
	"description": "A Modern Literate Programming Documentation Generator",
	"keywords": [
		"documentation",
		"docs",
		"generate",
		"generator",
		"literate",
		"javascript",
		"es6",
		"es2015",
		"ecmascript"
	],
	"author": "Gregory Wild-Smith",
	"version": "1.2.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/abritinthebay/docco-es.git"
	},
	"engines": {
		"node": ">=8.6.0"
	},
	"dependencies": {
		"@std/esm": "^0.26.0",
		"commander": "^2.15.1",
		"fs-extra": "^6.0.1",
		"highlight.js": "^9.12.0",
		"lodash.template": "^4.4.0",
		"marked": "^0.3.19"
	},
	"main": "./lib/index.js",
	"bin": {
		"docco": "./bin/docco"
	},
	"scripts": {
		"test": "jest --coverage --forceExit",
		"docs": "docco lib/*.js -l linear && cp ./docs/lib/index.html ./index.html",
		"posttest": "coveralls < coverage/lcov.info"
	},
	"@std/esm": {
		"esm": "js",
		"cjs": true
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^22.4.3",
		"babel-preset-env": "^1.7.0",
		"coveralls": "^3.0.1",
		"eslint": "^4.19.1",
		"eslint-plugin-babel": "^5.1.0",
		"jest": "^22.4.3"
	},
	"jest": {
		"coverageReporters": [
			"json",
			"lcov",
			"text"
		]
	}
}
